- pick_bans.each do |pick_ban|
  %li.list-group-item= present(pick_ban).status

- if match.confirmed?
  - present_collection(match.rounds).each do |round_p|
    - next unless round_p.has_outcome?
    %li.list-group-item
      = round_p.map.link

      .pull-right= round_p.result_description
- else
  - present_collection(match.rounds).each do |round_p|
    %li.list-group-item
      = round_p.map.link

      .pull-right Results Pending
